<?xml version="1.0" encoding="utf-8" ?>
<configuration>
  <configSections>
    <section name="nlog" type="NLog.Config.ConfigSectionHandler, NLog"/>
  </configSections>
  <startup>
    <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.5" />
  </startup>
  <nlog>
    <targets>
      <target name ="logfile" type="File" filename="./log/current.log" layout="[${longdate} ${level}]  ${message}" archiveFileName="./log/archive.{#####}.log" maxArchiveFiles="20" archiveAboveSize="1048576" archiveNumbering="Sequence" keepFileOpen="false" encoding="UTF-8" />
    </targets>
    <rules>
      <logger name="*" minlevel="Debug" writeTo="logfile" />
    </rules>
  </nlog>
</configuration>